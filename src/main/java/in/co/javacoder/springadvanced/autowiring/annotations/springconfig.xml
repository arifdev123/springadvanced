<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">
    
    <context:annotation-config/>

	<bean class="in.co.javacoder.springadvanced.autowiring.annotations.Address" name="address123"
		p:doorNumber="3" p:street="First Street 123" p:city="Chennai" />
	<bean class="in.co.javacoder.springadvanced.autowiring.annotations.Address" name="address1"
		p:doorNumber="3" p:street="First Street 111" p:city="Chennai" />

	<bean name="employee"
		class="in.co.javacoder.springadvanced.autowiring.annotations.Employee" />

</beans>